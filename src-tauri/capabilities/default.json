{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "store:default",
    {
      "identifier": "http:allow-openai",
      "allow": [
        {
          "url": "https://api.openai.com"
        }
      ]
    }
  ]
}
